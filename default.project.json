{
  "name": "Handlers",
  "tree": {
    "$className": "DataModel",
    "ReplicatedStorage": {
      "$ignoreUnknownInstances": true,
      "Common": {
        "$path": "src/shared"
      },
      "Events": {
        "$className": "Folder",
        "Input": {
          "$className": "RemoteEvent"
        },
        "Deactivate": {
          "$className": "BindableEvent"
        },
        "Hit": {
          "$className": "BindableEvent"
        }
      },
      "Functions": {
        "$className": "Folder",
        "getHitPos": {
          "$className": "RemoteFunction"
        }
      }
    },
    "ServerScriptService": {
      "$ignoreUnknownInstances": true,
      "Server": {
        "$path": "src/server"
      }
    },
    "StarterPlayer": {
      "StarterPlayerScripts": {
        "$ignoreUnknownInstances": true,
        "Client": {
          "$path": "src/client"
        }
      },
      "StarterCharacterScripts": {
        "$ignoreUnknownInstances": true,
        "Character": {
          "$path": "src/character"
        }
      }
    },
    "Workspace": {
      "$ignoreUnknownInstances": true,
      "$properties": {
        "FilteringEnabled": true
      },
      "Baseplate": {
        "$className": "Part",
        "$properties": {
          "Anchored": true,
          "Color": [
            0.38823,
            0.37254,
            0.38823
          ],
          "Locked": true,
          "Position": [
            0,
            -10,
            0
          ],
          "Size": [
            512,
            20,
            512
          ]
        }
      }
    },
    "Lighting": {
      "$ignoreUnknownInstances": true,
      "$properties": {
        "Ambient": [
          0,
          0,
          0
        ],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      },
      "$ignoreUnknownInstances": true
    }
  }
}